{
  "name": "walnut-admin-nestjs",
  "version": "0.1.0",
  "description": "An admin template, using `vue3`,`ts`,`vite`,`naive-ui`. Continuously updating...",
  "author": {
    "name": "zhaocl97",
    "email": "zhaocl97@foxmail.com",
    "url": "https://github.com/Zhaocl1997"
  },
  "license": "MIT",
  "homepage": "https://github.com/Zhaocl1997/walnut-admin-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zhaocl1997/walnut-admin-server.git"
  },
  "bugs": {
    "url": "https://github.com/Zhaocl1997/walnut-admin-server/issues"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "start:test": "NODE_ENV=staging node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:prod": "NODE_ENV=production pm2 start dist/main.js --name walnut-prod",
    "pm2:test": "NODE_ENV=staging pm2 start dist/main.js --name walnut-test",
    "update": "npm-check-updates"
  },
